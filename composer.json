{
    "name": "setnemo/php-telegram",
    "description": "Telegram API library for creating Telegram's Bots for business and fun.",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "setnemo",
            "email": "setnemo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.0",
        "symfony/http-foundation": "^6.0",
        "jms/serializer": "^3.17",
      "ext-dom": "*",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.7",
        "squizlabs/php_codesniffer": "*",
        "paquettg/php-html-parser": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Setnemo\\Telegram\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Unit\\": "tests/Unit/",
            "Tests\\Stubs\\": "tests/Stubs/"
        }
    },
    "version": "0.0.0"
}
